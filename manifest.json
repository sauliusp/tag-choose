{
  "manifest_version": 3,
  "name": "#TagChoose - Bookmark Manager with client-side AI",
  "version": "1.0.0",
  "description": "#TagChoose categorizes your bookmarks with client-side AI using Gemini Nano LLM within your browser.",
  "permissions": ["bookmarks", "tabs"],
  "key": "",
  "minimum_chrome_version": "138",
  "background": {
    "service_worker": "background/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "public/icon-16.png",
      "32": "public/icon-32.png",
      "48": "public/icon-48.png",
      "128": "public/icon-128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl + Shift + Y",
        "mac": "Command + Shift + Y"
      },
      "description": "Open the extension popup"
    }
  }
}
